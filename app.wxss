/**app.wxss**/
@import "./styles/iconfont.wxss";
/**微信小程序中不支持通配符**/
page,view,text,swiper,swiper-item,image,navigator{
padding : 0;
margin: 0;
box-sizing:border-box
}
/**主题颜色 通过变量实现
1 less中 存在 变量这个知识
原生的css和wxss也是支持变量
**/
page{
