/* pages/service/index.wxss */
.contain {
  min-height: 100%;
  overflow: hidden;
  overflow-y: hidden;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 750rpx;
  margin-top: 20rpx;
}
.userinfo-avatar {
  width: 160rpx;
  height: 160rpx;
}
.share{
  background: none; 
  width: 200px;
  height: 200px;
  padding: 0;
  border-radius: 50%;
}
.share .share-icon{
  width: 200px;
  height: 200px;
  border-radius: 50%;
}